<body class="log_in_turd"> 
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
   <!--    <div class="row">
        <div class="col-md-6 col-sm-6 col-md-offset-3 col-sm-offset-3"> -->
          <div id="sign_in_head">Sign in</div>
          <%= form_for(:session, url: sessions_path) do |f| %>

            <br>
            <%= f.label :email %>
            <br>
            <%= f.text_field :email %>

            <br>
            <br>

            <%= f.label :password %>
            <br>
            <%= f.password_field :password %>

            <br>
            <br>

            <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
          <% end %>
          <br>

          <p>New user? <%= link_to "Please sign up", signup_path %></p>
        </div>
      </div>
      </div>  <!-- modal header -->
    </div> <!-- modal content --> 
  </div> <!-- modal dialog -->  
</div>   <!-- div body -->