  <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
  <meta charset="utf-8">
  <style>
    html, body, #map-canvas {
      height: 100%;
      margin: 10px;
      padding: 30px
    }
  </style>
<div>
  <ul class="nav nav-pills">
    <li><%= link_to('Edit route', edit_route_path(@route)) %></li>
    <li><%= link_to('Delete route', @route, :method => :delete, :confirm => 'Are you sure?') %></li>
    <div class='spacer'></div>
  </ul>
</div>
<div class="row">
  <div class="col-md-5 col-sm-5">
    <p class="lead">Route:</p>
    <p><%= @route.starting_point %>  to <%= @route.end_point %> </p>
    <br>
  </div>
</div> 

  <div id="map-canvas"></div>


